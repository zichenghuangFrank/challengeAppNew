A,IAAA,CAAA,WAAA;AACA,QAAA,IAAA,CAAA,UAAA,GAAA,GAAA,CAAA,IAAA,CAAA,UAAA;AACA,QAAA,IAAA,CAAA,QAAA,GAAA,GAAA,CAAA,IAAA,CAAA,QAAA;AACA,QAAA,IAAA,CAAA,WAAA,GAAA,GAAA,CAAA,IAAA,CAAA,GAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OATA,EASA,KATA,CASA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OAXA;AAYA,KAhBA;AAiBA,IAAA,QAjBA,sBAiBA;AACA;AACA,UAAA,WAAA,GAAA,WAAA,KAAA,MAAA,GAAA,IAAA,GAAA,eAAA,GAAA,KAAA,WAAA,GAAA,IAAA,GAAA,eAAA,GACA,KAAA,WADA,GACA,IADA,GACA,YADA,GACA,KAAA,QADA,GACA,IADA,GACA,YADA,GACA,KAAA,QADA,GACA,IADA,GAEA,cAFA,GAEA,KAAA,UAFA,GAEA,IAFA,GAEA,eAFA,GAEA,KAAA,QAFA,CAFA,CAKA;;AACA,UAAA,cAAA,GAAA,IAAA,IAAA,CAAA,CAAA,WAAA,CAAA,EAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAAA,CAAA,CANA,CAOA;;AACA,UAAA,gBAAA,GAAA,KAAA,MAAA,GAAA,oBAAA;AACA,UAAA,YAAA,GAAA,QAAA,CAAA,aAAA,CAAA,GAAA,CAAA;AACA,MAAA,YAAA,CAAA,QAAA,GAAA,gBAAA,CAVA,CAWA;;AACA,MAAA,YAAA,CAAA,SAAA,GAAA,WAAA;AACA,MAAA,MAAA,CAAA,GAAA,GAAA,MAAA,CAAA,GAAA,IAAA,MAAA,CAAA,SAAA;AACA,MAAA,YAAA,CAAA,IAAA,GAAA,MAAA,CAAA,GAAA,CAAA,eAAA,CAAA,cAAA,CAAA;AACA,MAAA,YAAA,CAAA,KAAA,CAAA,OAAA,GAAA,MAAA;AACA,MAAA,QAAA,CAAA,IAAA,CAAA,WAAA,CAAA,YAAA;AACA,MAAA,YAAA,CAAA,KAAA;AACA;AAnCA;AAhBA,CAAA","sourcesContent":["<template>\n<!--    Use the row to show details about Shanghai weather-->\n    <div class=\"details\">\n        <Row type=\"flex\" justify=\"center\">\n            <i-col>\n                <h2>Basic Information</h2>\n            </i-col>\n        </Row>\n        <Row type=\"flex\" justify=\"center\">\n<!--            image reference: https://en.wikipedia.org/wiki/Shanghai-->\n            <img src=\"../assets/Shanghai.jpeg\" alt=\"Shanghai\" height=\"30%\" width=\"30%\">\n        </Row>\n        <Row type=\"flex\" justify=\"center\">\n            <i-col span=\"22\" offset=\"1\">\n                <Divider></Divider>\n            </i-col>\n        </Row>\n        <div class=\"information\">\n            <Row>\n                <i-col offset=\"5\" span=\"4\">\n                    city:{{cityen}}\n                </i-col>\n                <i-col span=\"4\">\n                    UpdateTime:{{update_time}}\n                </i-col>\n                <i-col span=\"4\">\n                    temperature:{{temperature}}\n                </i-col>\n                <i-col span=\"4\">\n                    humidity:{{humidity}}\n                </i-col>\n            </Row>\n            <br />\n            <Row>\n                <i-col offset=\"5\" span=\"4\">\n                    visibility:{{visibility}}\n                </i-col>\n                <i-col span=\"4\">\n                    pressure:{{pressure}}\n                </i-col>\n                <i-col span=\"4\">\n                    air_pm25:{{air_pm25}}\n                </i-col>\n            </Row>\n        </div>\n        <br/>\n        <Row type=\"flex\" justify=\"center\">\n            <i-col>\n                <Button type=\"primary\" @click=\"saveData\">Download</Button>\n            </i-col>\n        </Row>\n    </div>\n</template>\n\n<script>\n    export default {\n        name: \"details\",\n        data() {\n            return {\n                air_pm25: '',\n                cityen: '',\n                humidity: '',\n                pressure: '',\n                update_time: '',\n                visibility: '',\n                temperature: ''\n            }\n        },\n        created() {\n            this.getWeatherReport();\n        },\n        methods:{\n            getWeatherReport() {\n                let that = this;\n                let url = 'https://v0.yiketianqi.com/api?version=v61&appid=39425827&appsecret=cEgKfrw8&cityid=101020100';\n                this.$axios.get(url).then(function(res) {\n                    that.cityen= res.data.cityEn;\n                    that.humidity = res.data.humidity;\n                    that.pressure = res.data.pressure;\n                    that.update_time = res.data.update_time;\n                    that.visibility = res.data.visibility;\n                    that.air_pm25 = res.data.air_pm25;\n                    that.temperature = res.data.tem;\n                    console.log(res);\n                }).catch(function (error) {\n                    console.log(error);\n                });\n            },\n            saveData() {\n                // your CodeMirror textarea ID\n                var textToWrite = \"City: \" + this.cityen + '\\n' + \"Update time: \" + this.update_time + '\\n' + \"Temperature: \"\n                    + this.temperature + '\\n' + \"Humidity: \" + this.humidity + '\\n' + \"Pressure: \" + this.pressure + '\\n'\n                    + \"Visibility: \" + this.visibility + '\\n' + \"Air (pm2.5): \" + this.air_pm25;\n                // preserving line breaks\n                var textFileAsBlob = new Blob([textToWrite], { type: 'text/plain' });\n                // filename to save as\n                var fileNameToSaveAs = this.cityen + '_WeatherReport.txt';\n                var downloadLink = document.createElement('a');\n                downloadLink.download = fileNameToSaveAs;\n                // hidden link title name\n                downloadLink.innerHTML = 'LINKTITLE'\n                window.URL = window.URL || window.webkitURL;\n                downloadLink.href = window.URL.createObjectURL(textFileAsBlob);\n                downloadLink.style.display = 'none';\n                document.body.appendChild(downloadLink);\n                downloadLink.click();\n            }\n        }\n    }\n</script>\n\n<style scoped>\n    .details{\n        margin-top: 20px;\n    }\n</style>\n\n"],"sourceRoot":"src/components"}]}